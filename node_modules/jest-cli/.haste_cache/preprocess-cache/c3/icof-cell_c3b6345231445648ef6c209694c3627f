var Component, DOM, ICOFCell, React, createElement, div, input,
  bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; },
  extend = function(child, parent) { for (var key in parent) { if (hasProp.call(parent, key)) child[key] = parent[key]; } function ctor() { this.constructor = child; } ctor.prototype = parent.prototype; child.prototype = new ctor(); child.__super__ = parent.prototype; return child; },
  hasProp = {}.hasOwnProperty;

React = require('react');

Component = React.Component, DOM = React.DOM, createElement = React.createElement;

div = DOM.div, input = DOM.input;

ICOFCell = (function(superClass) {
  extend(ICOFCell, superClass);

  ICOFCell.prototype._cssPrefix = "icof-cell";

  ICOFCell.propTypes = {
    content: React.PropTypes.string
  };

  function ICOFCell(props) {
    this.render = bind(this.render, this);
    this._onChange = bind(this._onChange, this);
    ICOFCell.__super__.constructor.call(this, props);
    this.state = {
      content: this.props.content
    };
  }

  ICOFCell.prototype._onChange = function(event) {
    return this.setState({
      content: event.target.value
    });
  };

  ICOFCell.prototype.render = function() {
    return input({
      className: "" + this._cssPrefix,
      value: this.state.content,
      onChange: this._onChange
    });
  };

  return ICOFCell;

})(Component);

module.exports = ICOFCell;
